<config>
    <modules>
        <Polushkin_FirstModule>
            <version>1.0.0</version>
        </Polushkin_FirstModule>
    </modules>
    <frontend>
        <routers>
            <helloworld>
                <use>standard</use>
                <args>
                    <module>Polushkin_FirstModule</module>
                    <frontName>helloworld</frontName>
                </args>
            </helloworld>
        </routers>
        <layout>
            <updates>
                <helloworld>
                    <file>helloworld.xml</file>
                </helloworld>
            </updates>
        </layout>
    </frontend>
    <global>
        <blocks>
            <helloworld>
                <class>Polushkin_FirstModule_Block</class>
            </helloworld>
        </blocks>
        <models>
            <helloworld>
                <class>Polushkin_FirstModule_Model</class>
                <resourceModel>helloworld_resource</resourceModel>
            </helloworld>
            <helloworld_resource>
                <class>Polushkin_Firstmodule_Model_Resource</class>
                <entities>
                    <contact>
                        <table>ain_contact</table>
                    </contact>
                </entities>
            </helloworld_resource>
        </models>
        <resources>
            <helloworld_setup>
                <setup>
                    <module>Polushkin</module>
                </setup>
            </helloworld_setup>
        </resources>
    </global>
</config>